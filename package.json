{
    "name": "ghosttext",
    "displayName": "GhostText",
    "description": "ðŸ‘» Use your text editor to write in your browser. Everything you type in the editor will be instantly updated in the browser (and vice versa).",
    "version": "0.0.1",
    "publisher": "ghosttext",
    "engines": {
        "vscode": "^1.20.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.enableGhostText"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.enableGhostText",
                "title": "Enable GhostText"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.0.43",
        "@types/tmp": "0.0.33",
        "@types/ws": "^4.0.1",
        "tslint": "^5.8.0",
        "typescript": "^2.6.1",
        "vscode": "^1.1.6"
    },
    "dependencies": {
        "nodejs-websocket": "^1.7.1",
        "tmp": "0.0.33",
        "ws": "^4.0.0"
    }
}
